<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PantallaPrincipalMedico">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="197dp"
        android:layout_height="154dp"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logowhitedoc" />

    <Button
        android:id="@+id/button14"
        android:layout_width="@+id/button15"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="Perfil"
        app:layout_constraintEnd_toEndOf="@+id/button15"
        app:layout_constraintStart_toStartOf="@+id/button15"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        android:onClick="perfilMedico"/>

    <Button
        android:id="@+id/button15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Agendar citas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button14"
        android:onClick="perfilPaciente"/>

    <Button
        android:id="@+id/button16"
        android:layout_width="@+id/button15"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Calendario"
        app:layout_constraintEnd_toEndOf="@+id/button15"
        app:layout_constraintStart_toStartOf="@+id/button15"
        app:layout_constraintTop_toBottomOf="@+id/button15"
        android:onClick="pantallaCalendario"/>
</androidx.constraintlayout.widget.ConstraintLayout>